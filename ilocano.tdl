      NON-HEAD-DTR.SYNSEM.LOCAL.CAT.HEAD.MOD < [ L-PERIPH #periph ] > ].

same-periph-unary-phrase := unary-phrase &
    [ SYNSEM.L-PERIPH #periph,
      ARGS < [ SYNSEM.L-PERIPH #periph ] > ].

bare-np-phrase :+
    [ SYNSEM.LIGHT - ].

