;;; Semantically Empty Lexical Entries

comp_declarative_gtr := generator_rule &
  [ CONTEXT [ RELS <! [ ARG2 #h & handle ],
                    [ ARG0 #e & event ] !> ],
    FLAGS [ EQUAL < #e, #h >,
          TRIGGER "a_3" ]].

comp_interrogative_gtr := generator_rule &
  [ CONTEXT [ RELS <! [ ARG2 #h & handle ],
                    [ ARG0 #e & event ] !> ],
    FLAGS [ EQUAL < #e, #h >,
          TRIGGER "no_1" ]].

